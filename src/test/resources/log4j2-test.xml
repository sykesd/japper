<?xml version="1.0" encoding="UTF-8"?>
<Configuration>

    <Appenders>
        <Console name="STDOUT">
            <PatternLayout pattern="%d [%t] %-5p %c - %m%n" />
        </Console>
    </Appenders>

    <Loggers>
        <Logger name="org.dt.japper" level="debug" />

        <Root level="error">
            <AppenderRef ref="STDOUT" />
        </Root>
    </Loggers>

</Configuration>
